{
    "name": "yy-fps",
    "version": "0.3.0",
    "description": "Yet another FPS indicator with graph",
    "main": "index.js",
    "directories": {
        "doc": "docs"
    },
    "scripts": {
        "test": "budo --dir docs docs/code.js:index.js --live",
        "build": "browserify docs/code.js -o docs/index.js",
        "docs": "markdown-api README.md index.js",
        "prepare": "npm run build && npm run docs"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/davidfig/fps.git"
    },
    "keywords": [
