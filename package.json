    "license": "MIT",
    "bugs": {
        "url": "https://github.com/davidfig/fps/issues"
    },
    "homepage": "https://github.com/davidfig/fps#readme",
    "devDependencies": {
        "fork-me-github": "^1.0.2",
        "highlight.js": "^9.12.0",
        "pixi.js": "^4.5.6",
        "yy-renderer": "^1.3.3",
        "yy-loop": "^0.1.3",
        "yy-random": "^1.6.0",
        "pixi-ease": "^0.12.0",
        "clicked": "^1.3.0"
    },
    "dependencies": {
        "tinycolor2": "^1.4.1"
    }
